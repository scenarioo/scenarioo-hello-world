        $(&quot;#nameInput&quot;).change(function() {
            var name = $(&quot;#nameInput&quot;).val();
            var baseUrl = &quot;showName.html?name=&quot;;
            var withParam = baseUrl + name;
            $(&quot;#submitLink&quot;).attr(&quot;href&quot;, withParam);
        });
    });
&lt;/script&gt;

